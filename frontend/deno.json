{
  "imports": {
    "vue": "npm:vue@latest",
    "three": "npm:three@latest",
    "@tresjs/leches": "npm:@tresjs/leches@latest",
    "@polkadot/api": "npm:@polkadot/api@latest",
    "@polkadot/api-contract": "npm:@polkadot/api-contract@latest",
    "@polkadot/util": "npm:@polkadot/util@latest",
    "@polkadot/types/interfaces": "npm:@polkadot/types/interfaces",
    "@tresjs/core": "npm:@tresjs/core",
    "@tresjs/cientos":"npm:@tresjs/cientos",
    "@vue-router":"npm:vue-router@4",
    "marked": "npm:marked@latest"
  },
  "tasks": {
    "dev": "deno run -A --node-modules-dir npm:vite",
